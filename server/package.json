{
  "name": "upnext",
  "version": "2.0.0",
  "description": "UpNext is a party oriented real time vote-based playlist generation application with spotify integration",
  "private": true,
  "scripts": {
    "postinstall": "npm run generate-secret",
    "start": "node ./pack/server.js",
    "build": "npm run typescript && npm run webpack",
    "typescript": "tsc",
    "webpack": "webpack",
    "serve": "node ./dist/app.js",
    "dev": "concurrently \"npm:watch-ts\" \"npm:watch-pack\"\n",
    "generate-secret": "openssl ecparam -name prime256v1 -genkey -noout -out ./key.pem",
    "watch-ts": "tsc -w",
    "watch-pack": "webpack --watch",
    "serve-pack": "nodemon ./pack/server.js",
    "watch-node": "nodemon ./dist/app.js"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "homepage": "https://upnext.cool",
  "author": "Ethan Elliott <ethan.elliott@hotmail.ca>",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.19.0",
    "better-sqlite3": "^6.0.1",
    "body-parser": "^1.19.0",
    "chalk": "^4.0.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.0",
    "class-validator-jsonschema": "^1.3.0",
    "cors": "^2.8.5",
    "dddos": "^0.1.2",
    "deepcopy": "^2.0.0",
    "diskdb": "^0.1.17",
    "dotenv": "^4.0.0",
    "express": "^4.17.1",
    "express-status-monitor": "^1.2.10",
    "glob": "^7.1.6",
    "helmet": "^3.15.0",
    "jsonwebtoken": "^8.5.1",
    "kcors": "^2.2.2",
    "koa-bodyparser": "^4.3.0",
    "microframework-w3tec": "^0.6.3",
    "moment": "^2.24.0",
    "node-cron": "^2.0.3",
    "node-ts-uuid": "^1.0.5",
    "node-vibrant": "^3.1.5",
    "qrcode": "^1.4.4",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.0",
    "routing-controllers": "^0.8.0",
    "routing-controllers-openapi": "^1.8.0",
    "sha512": "0.0.1",
    "socket-controllers": "^0.0.5",
    "swagger-ui-express": "^4.1.2",
    "typedi": "latest",
    "tz-offset": "0.0.2",
    "uuid": "^3.3.3",
    "uws": "^10.148.1",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.14",
    "@types/express": "^4.17.2",
    "@types/express-status-monitor": "^1.2.0",
    "@types/helmet": "^0.0.43",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/moment-timezone": "^0.5.12",
    "@types/node": "^12.12.14",
    "@types/request": "^2.48.3",
    "@types/socket.io": "^2.1.4",
    "concurrently": "^3.5.1",
    "nodemon": "^1.18.10",
    "ts-loader": "^7.0.0",
    "typescript": "^3.7.2",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11"
  },
  "nodemonConfig": {
    "restartable": "rs",
    "ignore": [
      ".git",
      "node_modules/**/node_modules",
      "data/"
    ],
    "watch": [
      "dist/"
    ],
    "delay": 1000,
    "env": {
      "NODE_ENV": "development"
    },
    "ext": "js"
  }
}
