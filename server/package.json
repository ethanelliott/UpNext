{
  "name": "upnext",
  "version": "1.0.3",
  "description": "UpNext is a party oriented real time vote-based playlist generation application with spotify integration",
  "main": "./dist/src/app.js",
  "scripts": {
    "watch-ts": "tsc -w",
    "watch-node": "nodemon ./dist/src/app.js",
    "dev": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"yarn watch-ts\" \"yarn watch-node\"",
    "generate-secret": "openssl ecparam -name prime256v1 -genkey -noout -out ./key.pem"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "homepage": "https://upnext.cool",
  "author": "Ethan Elliott <ethan.elliott@hotmail.ca>",
  "dependencies": {
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.0",
    "class-validator-jsonschema": "^1.3.0",
    "cors": "^2.8.5",
    "dddos": "^0.1.2",
    "deepcopy": "^2.0.0",
    "diskdb": "^0.1.17",
    "dotenv": "^4.0.0",
    "express": "^4.17.1",
    "express-status-monitor": "^1.0.1",
    "glob": "^7.1.6",
    "helmet": "^3.15.0",
    "jsonwebtoken": "^8.5.1",
    "microframework-w3tec": "^0.6.3",
    "moment": "^2.24.0",
    "node-ts-uuid": "^1.0.5",
    "node-vibrant": "^3.1.5",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.0",
    "routing-controllers": "^0.8.0",
    "routing-controllers-openapi": "^1.8.0",
    "socket-controllers": "^0.0.5",
    "swagger-ui-express": "^4.1.2",
    "typedi": "latest",
    "uuid": "^3.3.3",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.14",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.2",
    "@types/express-status-monitor": "^1.2.0",
    "@types/helmet": "^0.0.43",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/moment-timezone": "^0.5.12",
    "@types/node": "^12.12.14",
    "@types/request": "^2.48.3",
    "@types/socket.io": "^2.1.4",
    "@types/winston": "^2.3.7",
    "concurrently": "^3.5.1",
    "nodemon": "^1.18.10",
    "typescript": "^3.7.2"
  },
  "nodemonConfig": {
    "restartable": "rs",
    "ignore": [
      ".git",
      "node_modules/**/node_modules",
      "data/"
    ],
    "watch": [
      "dist/"
    ],
    "delay": 1000,
    "env": {
      "NODE_ENV": "development"
    },
    "ext": "js"
  }
}
